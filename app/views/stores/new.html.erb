<h1> Create a Store Account </h1>

<%= form_for @store do |f| %>
    <%= f.label :username, "Username:" %>
    <%= f.text_field :username %> <br></br>
    <%= f.label :email, "Email:" %>
    <%= f.text_field :email %> <br></br>
    <%= f.label :password_digest, "Password:" %> 
    <%= f.text_field :password_digest %> <br></br>
    <%= f.submit %>
<% end %>