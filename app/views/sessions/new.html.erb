<h1> Log In </h1>

<%= form_for(:session, url: login_path) do |f| %>
    <%= f.label :username, "Username:" %>
    <%= f.text_field :username %> <br></br>
    <%= f.label :password_digest, "Password:" %> 
    <%= f.password_field :password_digest %> <br></br>
    <%= f.submit "Log In"%>
<% end %>