<h1> Welcome <%= current_user.username %>! </h1>

<h2> Edit your Account </h2>

<%= form_for @user do |f| %>
    <%= f.label :email, "Email:" %>
    <%= f.text_field :email %> <br></br>
    <%= f.label :password, "Password:" %> 
    <%= f.password_field :password %> <br></br>
    <%= f.submit %>
<% end %>